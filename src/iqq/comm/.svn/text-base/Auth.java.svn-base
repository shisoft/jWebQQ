package iqq.comm;

import iqq.model.Member;

/**
 *
 * @author chenzhihui
 */
public class Auth {

    private static int clientid = 73937875;
    private static String psessionid;
    private static String ptwebqq;
    private static String vfwebqq;
    private static String skey;
    private static Member member = new Member();

    public static Member getMember() {
        return member;
    }

    public static void setMember(Member member) {
        Auth.member = member;
    }
    
    public static int getClientid() {
        return clientid;
    }

    public static void setClientid(int clientid) {
        Auth.clientid = clientid;
    }

    public static String getPsessionid() {
        return psessionid;
    }

    public static void setPsessionid(String psessionid) {
        Auth.psessionid = psessionid;
    }

    public static String getPtwebqq() {
        return ptwebqq;
    }

    public static void setPtwebqq(String ptwebqq) {
        Auth.ptwebqq = ptwebqq;
    }

    public static String getSkey() {
        return skey;
    }

    public static void setSkey(String skey) {
        Auth.skey = skey;
    }

    public static String getVfwebqq() {
        return vfwebqq;
    }

    public static void setVfwebqq(String vfwebqq) {
        Auth.vfwebqq = vfwebqq;
    }
}
